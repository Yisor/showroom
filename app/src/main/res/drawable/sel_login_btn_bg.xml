<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/btn_gray_react" android:state_enabled="false"/>
    <item android:drawable="@drawable/btn_blue_react" android:state_enabled="true" android:state_pressed="true"/>
    <item android:drawable="@drawable/btn_blue_react" android:state_enabled="true" android:state_pressed="false"/>
    <!-- pressed -->
    <item android:drawable="@drawable/btn_blue_react" android:state_focused="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/btn_blue_react"/>
    <!-- default -->
</selector>